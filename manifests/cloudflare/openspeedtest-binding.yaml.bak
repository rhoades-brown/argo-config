apiVersion: networking.cfargotunnel.com/v1alpha1
kind: TunnelBinding
metadata:
  name: openspeedtest-tunnel-binding
  annotations:
    argocd.argoproj.io/sync-options: SkipDryRunOnMissingResource=true
subjects:
  - name: immich-server
    kind: Service
    spec:
      fqdn: openspeedtest.rhoades-brown.co.uk
      protocol: http
      target: http://openspeedtest.openspeedtest:3000
tunnelRef:
  kind: ClusterTunnel
  name: cluster-tunnel
